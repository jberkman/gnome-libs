module GNOME {
  interface object {
    /**
     * ref:
     *
     * increments the reference count
     */
    oneway void ref ();
    /**
     * unref:
     * 
     * decrements the reference count
     */
    oneway void unref ();

    Object query_interface (in string repoid);
  };
};

#ifndef __LIBGNORBA_COMPILATION
#pragma inhibit pop
#endif
